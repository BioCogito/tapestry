<h1>New answer option</h1>

<% form_for :answer_option, @answer_option,
            :url => admin_content_area_exam_exam_version_exam_question_answer_options_path(
              @content_area,
              @exam,
              @exam_version,
              @exam_question),
            :html => { :method => :post } do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', admin_content_area_exam_exam_version_exam_question_answer_options_path(@content_area, @exam, @exam_version, @exam_question) %>
