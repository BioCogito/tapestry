<%= form_for [:admin, @admin_oauth_service] do |f| %>
  <% if @admin_oauth_service.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@admin_oauth_service.errors.count, "error") %> prohibited this admin_oauth_service from being saved:</h2>

      <ul>
      <% @admin_oauth_service.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :scope %><br />
    <%= f.text_field :scope, :size => 100 %>
  </div>
  <div class="field">
    <%= f.label :consumerkey, 'Consumer key' %><br />
    <%= f.text_field :consumerkey %>
  </div>
  <div class="field">
    <%= f.label :privatekey, 'Private key' %><br />
    <%= f.text_area :privatekey, :rows => 16, :cols => 100 %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
