<h2>Update a designated proxy</h2>

<%= error_messages_for :named_proxy, :header_message => 'Error: data could not be saved.', :message => nil %>

<%= form_for @named_proxy do |f| %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email, 'E-mail address' %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>

<%= link_to 'Back', named_proxies_path %>
