<h1>New exam definition</h1>

<% form_for [:admin, @content_area, ExamDefinition.new] do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', admin_content_area_exam_definitions_path(@content_area) %>
